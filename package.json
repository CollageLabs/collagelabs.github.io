{
  "name": "collagelabs.org",
  "version": "0.1.1",
  "description": "Collage Labs website",
  "author": "Luis Alejandro Mart√≠nez Faneyth <luis@luisalejandro.org>",
  "repository": "CollageLabs/collagelabs.org",
  "license": "GPL-3.0-or-later",
  "scripts": {
    "clean:project": "rimraf ./_site ./assets",
    "webpack:dev": "webpack --env=dev --progress --profile --colors",
    "webpack:prod": "cross-env NODE_ENV=production webpack --env=prod --progress --profile --colors",
    "webpack:pwa": "cross-env NODE_ENV=production webpack --env=pwa --progress --profile --colors",
    "webpack:optim": "cross-env NODE_ENV=production webpack --env=optim --progress --profile --colors",
    "jekyll:dev": "cross-env JEKYLL_ENV=development concurrently \"webpack-dev-server --env=dev\" \"bundle exec jekyll serve --config=_config.yml,_config.development.yml\"",
    "jekyll:prod": "cross-env JEKYLL_ENV=production bundle exec jekyll build --config=_config.yml,_config.production.yml",
    "jekyll:pwa": "cross-env JEKYLL_ENV=pwa bundle exec jekyll build --config=_config.yml,_config.production.yml",
    "sw": "sw-precache --config=config/sw.config.js --verbose",
    "build": "yarn run clean:project && yarn run webpack:prod && yarn run jekyll:prod",
    "build:optim": "yarn run clean:project && yarn run webpack:prod && yarn run jekyll:prod && yarn run webpack:optim",
    "build:pwa": "yarn run clean:project && yarn run webpack:pwa && yarn run jekyll:pwa && yarn run sw",
    "build:pwa:optim": "yarn run clean:project && yarn run webpack:pwa && yarn run jekyll:pwa && yarn run webpack:optim && yarn run sw",
    "serve:dist": "yarn run build:pwa:optim && http-server _site",
    "serve:dev": "yarn run clean:project && yarn run webpack:dev && yarn run jekyll:dev",
    "snyk-protect": "npx snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "compact": false
  },
  "devDependencies": {
    "@babel/core": "7.8.0",
    "@babel/preset-env": "7.9.0",
    "autoprefixer": "9.7.5",
    "babel-loader": "8.0.6",
    "browser-sync": "2.26.7",
    "browser-sync-webpack-plugin": "2.2.2",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.0.1",
    "copy-webpack-plugin": "5.0.5",
    "cross-env": "7.0.2",
    "css-loader": "3.4.2",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.10",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-loader": "3.0.0",
    "eslint-plugin-import": "2.20.1",
    "file-loader": "5.0.2",
    "html-critical-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "4.0.4",
    "http-server": "0.12.1",
    "imagemin-mozjpeg": "8.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "postcss-advanced-variables": "3.0.0",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.1.2",
    "postcss-preset-env": "6.7.0",
    "postcss-scss": "2.0.0",
    "rimraf": "3.0.0",
    "sass-loader": "8.0.0",
    "style-loader": "1.0.0",
    "svg-sprite-loader": "4.1.6",
    "svg-transform-loader": "2.0.7",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "sw-precache": "5.2.1",
    "uglifyjs-webpack-plugin": "2.1.3",
    "url-loader": "3.0.0",
    "webapp-webpack-plugin": "2.7.1",
    "webpack": "4.41.5",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.1.0",
    "webpack-pwa-manifest": "4.1.1",
    "yarn": "1.22.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.13.0",
    "bootstrap": "4.3.1",
    "cookieconsent": "3.1.1",
    "d3": "5.15.1",
    "expose-loader": "0.7.5",
    "jquery": "3.4.1",
    "normalize.css": "8.0.1",
    "popper.js": "1.15.0",
    "puppeteer": "2.0.0",
    "slick-carousel": "1.8.1",
    "snyk": "1.305.0"
  },
  "snyk": true
}
