{
  "name": "collagelabs.org",
  "version": "0.2.4",
  "description": "Collage Labs website",
  "author": "Luis Alejandro Mart√≠nez Faneyth <luis@luisalejandro.org>",
  "license": "GPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CollageLabs/collagelabs.org.git"
  },
  "bugs": {
    "url": "https://github.com/CollageLabs/collagelabs.org/issues"
  },
  "homepage": "https://github.com/CollageLabs/collagelabs.org#readme",
  "scripts": {
    "clean:project": "rimraf ./_site ./assets ./functions/*.js",
    "webpack:dev": "webpack --env=dev --progress --profile --colors",
    "webpack:prod": "cross-env NODE_ENV=production webpack --env=prod --progress --profile --colors",
    "webpack:pwa": "cross-env NODE_ENV=production webpack --env=pwa --progress --profile --colors",
    "webpack:optim": "cross-env NODE_ENV=production webpack --env=optim --progress --profile --colors",
    "jekyll:dev": "cross-env JEKYLL_ENV=development concurrently \"webpack-dev-server --env=dev\" \"bundle exec jekyll serve --config=_config.yml,_config.development.yml\"",
    "jekyll:prod": "cross-env JEKYLL_ENV=production bundle exec jekyll build --config=_config.yml,_config.production.yml",
    "jekyll:pwa": "cross-env JEKYLL_ENV=pwa bundle exec jekyll build --config=_config.yml,_config.production.yml",
    "sw": "sw-precache --config=config/sw.config.js --verbose",
    "build": "yarn run clean:project && yarn run webpack:prod && yarn run jekyll:prod && yarn run build:lambda",
    "build:optim": "yarn run clean:project && yarn run webpack:prod && yarn run jekyll:prod && yarn run build:lambda && yarn run webpack:optim",
    "build:pwa": "yarn run clean:project && yarn run webpack:pwa && yarn run jekyll:pwa && yarn run build:lambda && yarn run sw",
    "build:pwa:optim": "yarn run clean:project && yarn run webpack:pwa && yarn run jekyll:pwa && yarn run build:lambda && yarn run webpack:optim && yarn run sw",
    "serve:dist": "yarn run clean:project && yarn run build:pwa:optim && http-server _site",
    "serve:dev": "yarn run clean:project && yarn run webpack:dev && yarn run jekyll:dev",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect && yarn run snyk-protect",
    "start:lambda": "netlify-lambda serve lambda",
    "build:lambda": "netlify-lambda build lambda"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "compact": false
  },
  "devDependencies": {
    "@babel/core": "7.11.0",
    "@babel/preset-env": "7.10.4",
    "autoprefixer": "9.8.5",
    "axios": "0.19.2",
    "babel-loader": "8.1.0",
    "browser-sync": "2.26.12",
    "browser-sync-webpack-plugin": "2.2.2",
    "classlist-polyfill": "1.2.0",
    "clean-webpack-plugin": "3.0.0",
    "concurrently": "5.2.0",
    "cross-env": "7.0.2",
    "css-loader": "4.0.0",
    "css-mqpacker": "7.0.0",
    "cssnano": "4.1.10",
    "dotenv": "8.2.0",
    "encoding": "0.1.13",
    "eslint": "7.6.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-loader": "4.0.2",
    "eslint-plugin-import": "2.22.0",
    "favicons-webpack-plugin": "4.2.0",
    "file-loader": "6.0.0",
    "googleapis": "56.0.0",
    "html-critical-webpack-plugin": "2.1.0",
    "html-webpack-plugin": "4.3.0",
    "http-server": "0.12.3",
    "imagemin-mozjpeg": "9.0.0",
    "imagemin-webpack-plugin": "2.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "netlify-lambda": "2.0.1",
    "node-sass": "4.14.1",
    "postcss-advanced-variables": "3.0.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-nested": "4.2.3",
    "postcss-preset-env": "6.7.0",
    "postcss-scss": "2.1.1",
    "rimraf": "3.0.2",
    "sass-loader": "9.0.2",
    "style-loader": "1.2.1",
    "svg-sprite-loader": "5.0.0",
    "svg-transform-loader": "2.0.13",
    "svg-url-loader": "6.0.0",
    "svgo": "1.3.2",
    "svgo-loader": "2.2.1",
    "sw-precache": "5.2.1",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.0",
    "webpack": "4.44.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-merge": "5.1.0",
    "webpack-pwa-manifest": "4.2.0",
    "yarn": "1.22.4"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.14.0",
    "bootstrap": "4.5.0",
    "cookieconsent": "3.1.1",
    "d3": "5.16.0",
    "expose-loader": "1.0.0",
    "jquery": "3.5.1",
    "normalize.css": "8.0.1",
    "popper.js": "1.16.1",
    "puppeteer": "5.2.1",
    "slick-carousel": "1.8.1",
    "snyk": "^1.367.0"
  },
  "snyk": true
}
